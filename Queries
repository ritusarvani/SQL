1)
SELECT *
FROM emp;

2)
SELECT *
FROM emp
WHERE deptno = 10;

3)
SELECT *
FROM emp
WHERE deptno != 30;

4)
SELECT count(*)
FROM emp;

5)
SELECT *
FROM emp
ORDER BY sal DESC;

6)
SELECT *
FROM emp
FETCH FIRST 5 ROWS ONLY;

7)
SELECT *
FROM emp
ORDER BY deptno,sal DESC;

8)
SELECT *
FROM emp
WHERE hiredate BETWEEN '31-DEC-80' and '01-JAN-82';

10)
SELECT *
FROM emp
WHERE (mgr = 7698 or mgr = 7566) and sal > 1500;

11)
SELECT *
FROM emp
WHERE sal < ALL (SELECT sal
             FROM emp
             WHERE ename = 'FORD');

12)
SELECT ROWNUM,emp.*
FROM emp;

13)
SELECT current_date
FROM dual;

14)
SELECT DISTINCT job
FROM emp;

16)
SELECT ename,dname
FROM emp,dept
WHERE emp.deptno = dept.deptno and comm IS NOT NULL;

17)
SELECT empno, ename, sal, sal*1.5 AS salary
FROM emp;

18)
select 	e1.ename ename,e2.ename mgrname
from emp e1,emp e2
where e1.mgr = e2.empno;

19)
SELECT DISTINCT dname
FROM emp, dept
WHERE emp.deptno = dept.deptno and sal >ALL (SELECT avg(sal)
                                          FROM emp, dept
                                          GROUP BY dname);

20)
SELECT ename
FROM emp
WHERE length(ename) = 5;

21)
SELECT ename
FROM emp
WHERE ename LIKE 'J%S';

22)
SELECT ename
FROM emp
WHERE deptno NOT IN (10,20,40);

23)
SELECT ename
FROM emp
WHERE job NOT IN ('PRESIDENT','MANAGER');

24)
SELECT max(sal)
FROM emp;

25)
SELECT avg(sal)
FROM emp
WHERE job = 'SALESMAN';

26)
SELECT ename
FROM emp
WHERE ename LIKE '_L%';

28)
SELECT deptno
FROM emp
GROUP BY deptno
HAVING count(empno) = 3;

29)
SELECT sum(sal),dname
FROM emp,dept
WHERE emp.deptno = dept.deptno
GROUP BY dname;

30)
SELECT dname, max(sal), min(sal)
FROM emp NATURAL JOIN dept
GROUP BY dname;

31)
SELECT ename, empno, dname, loc
FROM emp NATURAL JOIN dept;

